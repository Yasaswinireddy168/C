#include<stdio.h>
int main()
{
    int i,n,key,a[50];
    printf("Enter size of an array:\n");
    scanf("%d",&n);
    printf("Enter %d elements:\n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("Enter element to be searched:\n");
    scanf("%d",&key);
    for(i=0;i<n;i++)
    {
        if(key==a[i])
        {
            printf("Element found at position:%d\n",i+1);
            break;
        }
    }
    if(i==n)
    {
        printf("No element found\n");
    }
    return 0;
}
