#include<stdio.h>
int main()
{
    int a[50],i,max,min,n;
    printf("enter n value");
    scanf("%d",&n);
    printf("enter %d elements\n",n);
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    min=max=a[0];
    for(i=0;i<n;i++)
    {
        if(a[i]>max)
        max=a[i];
        if(a[i]<min)
       min=a[i];
    }
    printf("Min=%d\n",min);
    printf("Max=%d\n",max);
    return 0;
}
